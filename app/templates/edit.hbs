<form class="question-title" onsubmit={{action "editQuestion"}}>
  {{#link-to "questions"}}
    <button class="primary-btn">Back</button>
  {{/link-to}}
  <br><br>
  <div class="form-group">
    <label for="exampleFormControlInput1">Title</label>
    {{input type="text" class="form-control" placeholder="What is Ember?" value=model.title }}
  </div>
  <div class="form-group">
    <label for="exampleFormControlTextarea1">Body</label>
    {{textarea class="form-control" id="exampleFormControlTextarea1" rows="3" value=model.body }}
  </div>
  <button type="submit" class="btn btn-success">Create</button>
</form>
